	<parameter name="SpecialCondition" class="java.lang.String"/>
	<parameter name="parPolicyNumber" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
P.POLH_SYS_ID, --5286
P.POLH_SRNUM,
P.POLH_POLNUM,
FN_GET_POLICY_FORMAT_CLAIM(FN_GET_POLICY_FORMAT_V2(P.POLH_POLNUM,P.POLH_COB,P.POLH_EXTNUM)) AS POLH_DISPLAY_POLNUM,
(SELECT B.CVBR_BRANCH_NAME FROM RAIMS.IBM_PARTY_BRANCH B WHERE CVBR_BRANCH_CODE IN(P.POLH_DEBIT_PARTY)) AS INT_NAME,
--(SELECT B.CVBR_BRANCH_NAME FROM IBM_PARTY_BRANCH B WHERE CVBR_BRANCH_CODE IN('BA000008')) AS INT_NAME,
P.POLH_DEBIT_PARTY AS INT_CODE,
UPPER(P.POLH_ASSRDNM) AS INSURED_NAME,
P.POLH_CLIENT AS INSURED_CODE,
replace(replace(P.POLH_ASSRD_ADDR,chr(10),' '),chr(13),' ') AS ADDRESS,
'N/A' AS BUSINESS_REG_NO,
--'?' AS BUSINESS_OCCUPATION,
P.POLH_INDUSTRY AS BUSINESS_OCCUPATION,
(SELECT I.IND_DESC FROM RAIMS.INM_MST_IND I WHERE I.IND_CODE=P.POLH_INDUSTRY) AS BUSINESS_OCCUPATION_DESC,
--P.POLH_DAYS AS PERIOD_OF_INSURANCE,
TO_CHAR(P.POLH_ISSDT,'DD-MON-YYYY')ISSUE_DATE,
TO_CHAR(P.POLH_STPERIOD,'DD-MON-YYYY')EFFECTIVE_DATE,
TO_CHAR(P.POLH_ENDPERIOD,'DD-MON-YYYY') EXPIRY_DATE,
O2.PBLK_LOC_DESC||O2.PBLK_LOC_ADD AS LOCATION,
O2.PBLK_TARIFF_DESC AS OCC_CONTRUCTION_TYPE,
(SELECT OBJ_DESC FROM RAIMS.INM_MST_OBJ WHERE OBJ_CODE=O3.PINT_OBJECT) AS OCC_CONTRUCTION_TYPE2,
--I.PINT_DESC AS ITEM_INTEREST_DETAIL,
--'?' AS COVER_DESCRIPTION,
--ROUND(P.POLH_PREM_FC,2) AS PREMIUM,
--'1.00' AS ADMIN_FEE,
--(P.POLH_PREM_FC+1) AS TOTAL,
--'?' AS DEDUCTIBLE,
--'' AS SPECIAL_CONDITION,
--P.*
O2.PBLK_SYS_ID,O2.PBLK_BLKRSK,O2.PBLK_SYS_ID AS O2_PBLK_SYS_ID,O2.PBLK_BLKRSK AS O2_PBLK_BLKRSK,--O2.PBLK_TARIFF_DESC,
O3.PINT_SYS_ID,O3.PINT_BLKRSK,O3.PINT_BLK_SYS_ID,O3.PINT_COB,O3.PINT_SEQNUM,O3.PINT_DESC,O3.PINT_OBJECT,O3.PINT_SI_FC,
ROUND(ALIST.PINR_EFF_RT,4) AS PINR_EFF_RT, ROUND(ALIST.PINR_PERIL_RT,3) AS PINR_PERIL_RT,
--ALIST.PINR_EFF_RT AS PINR_EFF_RT, ALIST.PINR_PERIL_RT AS PINR_PERIL_RT,
(O3.PINT_SI_FC * ROUND(ALIST.PINR_PERIL_RT,3))/100 AS PREMIUM,
CASE when ((O3.PINT_SI_FC * ROUND(ALIST.PINR_PERIL_RT,3))/100) <70 THEN 70 ELSE (O3.PINT_SI_FC * ROUND(ALIST.PINR_PERIL_RT,3))/100 END AS COND_PREMIUM
--(SELECT PINT_SYS_ID FROM INT_UW_PINT WHERE PINT_SYS_ID='709') AS MIN_SYS,
--(SELECT PINT_SYS_ID FROM INT_UW_PINT WHERE PINT_SYS_ID=(SELECT MIN(PINT_SYS_ID) FROM INT_UW_PINT )) AS MIN_SYS2
--R4.PINR_SEQNUM,R4.PINR_VALUE,R4.PINR_EFF_RT,R4.PINR_PERIL_RT
,P.POLH_EMPLOYEE
FROM RAIMS.INT_UW_POLH P
INNER JOIN RAIMS.INT_UW_PBLKRSK O2 ON P.POLH_SYS_ID = O2.PBLK_POLH_SYS_ID
INNER JOIN RAIMS.INT_UW_PINT    O3 ON O3.PINT_BLK_SYS_ID = O2.PBLK_SYS_ID AND O3.PINT_BLKRSK=O2.PBLK_BLKRSK
INNER JOIN (
    SELECT R4.PINR_CODE, R4.PINR_PINT_SYS_ID,R4.PINR_SYS_ID,R4.PINR_SEQNUM,R4.PINR_VALUE,R4.PINR_EFF_RT,R4.PINR_PERIL_RT 
    FROM RAIMS.INT_UW_PINT_RT R4
    --where R4.PINR_PINT_SYS_ID IN ('709','710','711')
    )
ALIST ON ALIST.PINR_PINT_SYS_ID=O3.PINT_SYS_ID  AND ALIST.PINR_CODE=O3.PINT_COB AND ALIST.PINR_PINT_SYS_ID=O3.PINT_SYS_ID
--INNER JOIN RAIMS.INT_UW_PCOND O4 ON P.POLH_SYS_ID=O4.PCOND_POLH_SYS_ID --'5286'; --OK
WHERE P.POLH_POLNUM = $P{parPolicyNumber}
--WHERE P.POLH_POLNUM='P01FI00200940/000/00']]>
	</queryString>
