	<parameter name="parPAYMENT_NUMBER" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT
	t1.PAPPRH_INTMTNH_CLMNUM CLAIMNUMBER,	--Claim Number
	t1.PAPPRH_INTMTNH_POLNUM POLICYNUMBER,  	--Policy Number
	t1.PAPPRH_NUMBER PAYMENTNUMBER, 			--Payment Number
	T1.PAPPRH_TOT_APPR_FC AMOUNTPAYMENT,		--Amount Payment
	t1.PAPPRH_EXCHDT DATEOFPAYMENT,			--Date of Payment
	t1.PAPPRH_NAME PAYEENAME,				--Pay to No Name
	t1.PAPPRH_ADDR1 ADDRESS1,			--Address 1
	t1.PAPPRH_ADDR2 ADDRESS2,			--Address 2
	t1.PAPPRH_ADDR3 ADDRESS3,			--Address 3
	t1.PAPPRH_FTR_NAME FEATUREDETAIL,			--Feature Deatail
	t1.PAPPRH_FTR_CODE FEATURENO,			--Feature No
	t1.PAPPRH_REM REMARKPAYMENT,			--Remark Payment
	t5.CLS_DESC PRODUCTCODE,				--Product Code 
	t1.PAPPRH_PAY_TYPE PAYMENTTYPE,			--Payment Type
	CASE t1.PAPPRH_PAY_TYPE 
		WHEN 'C' THEN 'Customer'
	END AS Customer,
	--t2.POLH_BSOURCE BROKER,			--Broker/Agent
	t3.PINT_INSURED INSUREDNAME,			--INSUREDNAME
	t4.CLMH_NATURE CAUSEOFLOSS,				--Cause of loss
	t6.NOL_DESC NATUREOFLOSS,			--Nature of Loss
	t4.CLMH_LOSS_DATE DATEOFLOSS,			--Date of loss
	t4.CLMH_TXN_DATE TRANSACTIONDATE,						--Transaction Date
	t4.CLMH_REM NARRATION,				--Naration
	t4.CLMH_PLACE_ACCIDENT PLACEOFACCIDENT,		--Place of Accident
	t3.PINT_EMP_ID EMPLOYEENO,				--Employee No.
	t4.CLMH_LNKNUM EVENT,				--Event
	t1.PAPPRH_PAY_TO PAYEENO,			--Pay to No
	t2.POLH_ASSRDNM ASSUREDNAME,			--Assured name
	t1.PAPPRH_TOT_EST_FC ESTIMATEAMOUNT,	--Estimate Amount
	t1.PAPPRH_ADJ_FEE_FC CLAIMEXPNSE,		--Claim Expense
	t1.PAPPRH_TOT_APPR_FC APPROVALAMOUNT,	--Approval Amount
	t1.PAPPRH_TOT_DED_FC DEDUCT_AMOUNT,		--Deductible Amount
	t2.POLH_STPERIOD STARTPERIOD,			--Start Peroid 
	t2.POLH_ENDPERIOD ENDPEROID,				--End Peroid
	t2.POLH_ACCPT_TYPE,
		CASE t2.POLH_ACCPT_TYPE
		WHEN 'D' THEN 'Direct'
	END AS BROKER_AGENT
FROM
	RAIMS.INT_CL_PAY_APPR_HEAD t1,
	RAIMS.INT_UW_POLH t2,
	RAIMS.INT_UW_PINT_MED t3,
	RAIMS.INT_CLM_HEAD t4,
	RAIMS.INM_MST_CLASS t5,
	RAIMS.INM_MST_NOL t6
WHERE 
	t1.PAPPRH_INTMTNH_POLNUM = t2.POLH_POLNUM
AND	t2.POLH_SYS_ID = t3.PINT_POLH_SYS_ID
AND t1.PAPPRH_COB = t5.CLS_CODE
AND t3.PINT_SYS_ID = t1.PAPPRH_INT_SYS_ID
AND t1.PAPPRH_INTMTNH_CLMNUM = t4.CLMH_CLMNUM
AND t4.CLMH_NOL_CODE = t6.NOL_CODE
AND t2.POLH_POLNUM = t4.CLMH_POLNUM
AND t3.PINT_INSURED IS NOT NULL
AND t2.POLH_ASSRDNM IS NOT NULL
AND t1.PAPPRH_NUMBER = $P{parPAYMENT_NUMBER}
--AND t1.PAPPRH_NUMBER = 'A01HC002000026'


--FETCH first 1 ROW ONLY]]>
	</queryString>
