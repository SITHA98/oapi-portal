	<parameter name="parClaimNumber" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT
	t6.CLMH_CLMNUM CLAIMNUMBER,					--Claim Number
	t6.CLMH_POLNUM POLICYNUMBER,  				--Policy Number
	t2.POLH_DEBIT_PARTY BROKERAGENT,			--Broker Agent
	t2.POLH_ASSRDNM INSUREDNAME,				--InsuredName
	t2.POLH_STPERIOD STARTPERIOD,				--Start Peroid
	t2.POLH_ENDPERIOD STOPPEROID,				--End Peroid
	t6.CLMH_COB PRODUCTCODE,					--product code
	CASE t6.CLMH_COB
       WHEN 'PA00' THEN 'PERSONAL ACCIDENT GROUP'
	END AS PRODUCT,
	t6.CLMH_NATURE CAUSEOFLOSS,					--Cause of loss
	t6.CLMH_LOSS_DATE DATEOFLOSS,				--Date of loss
	t6.CLMH_REP_DATE REPORTINGDATE,				--ReportingDate
	t6.CLMH_TXN_DATE TRANSACTIONDATE,			--Transaction Date
	t6.CLMH_REM NARRATION,						--Naration
	t6.CLMH_PLACE_ACCIDENT PLACEOFACCIDENT,		--Place of Accident
	t6.CLMH_LNKNUM EVENT,						--Event
	t5.CLMF_FTR_NAME FEATURENAME,				--Feature Name
	t2.POLH_BSOURCE_ID BROKER,							--Broker
	t2.POLH_DEBIT_PARTY BROKER_CODE,
	t5.CLMF_EST_FC ESTIMATEAMOUNT,				--Estimate Amount	
    NVL(t5.CLMF_DED_FC, '0') AS DEDUCTIBLE,--Deductible
	t6.CLMH_ADJ_FEE_FC CLAIMEXPENSE,			--Claim Amount Expense
	t6.CLMH_CRE_DT CREATED_DATE,				--created_date
	t6.CLMH_APPROVAL_DT	APPROVED_DATE,			--Approved Date 
	t8.NOL_DESC NATURE_OF_LOSS,       --Nature of Loss Code
	(SELECT U.USER_ID FROM RAIMS.MMGR_USERS U WHERE U.USER_INIT=t6.CLMH_CRE_USER_INIT) AS CREATED_BY,
	(SELECT U.USER_ID FROM RAIMS.MMGR_USERS U WHERE U.USER_INIT=t6.CLMH_APPROVAL_BY) AS APPROVED_BY 
FROM 
	RAIMS.INT_UW_POLH t2,
	RAIMS.INT_UW_PINT_MED t4,
	RAIMS.INT_CLM_FEATURE t5,
	RAIMS.MMGR_USERS t7,
	RAIMS.INT_CLM_HEAD t6,
	RAIMS.INM_MST_NOL t8
	
	

WHERE 
	t6.CLMH_POLH_SYS_ID = t2.POLH_SYS_ID
	AND t7.USER_INIT = t6.CLMH_CRE_USER_INIT
	AND t6.CLMH_SYS_ID = t5.CLMF_CLMH_SYS_ID
	AND t2.POLH_POLNUM = t6.CLMH_POLNUM
	AND t6.CLMH_POLNUM = t2.POLH_POLNUM
	AND t6.CLMH_NOL_CODE = t8.NOL_CODE
	AND t2.POLH_DEBIT_PARTY IS NOT NULL
    AND t2.POLH_ASSRDNM IS NOT NULL
--	AND t6.CLMH_CLMNUM = 'C01PA00200083'
	AND t6.CLMH_CLMNUM = $P{parClaimNumber}]]>
	</queryString>
